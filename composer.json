{
	"name": "kuriousagency/commerce-alipay",
	"description": "Alipay integration for Craft Commerce",
	"type": "craft-plugin",
	"version": "1.0.0",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"commerce alipay"
	],
	"support": {
		"docs": "https://github.com/KuriousAgency/commerce-alipay/blob/master/README.md",
		"issues": "https://github.com/KuriousAgency/commerce-alipay/issues"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Kurious Agency",
			"homepage": "https://kurious.agency"
		}
	],
	"require": {
		"craftcms/cms": "^3.0",
		"craftcms/commerce": "^2.0|^3.0"
	},
	"autoload": {
		"psr-4": {
			"kuriousagency\\commerce\\alipay\\": "src/"
		}
	},
	"extra": {
		"name": "Alipay for Craft Commerce",
		"handle": "commerce-alipay",
		"hasCpSettings": false,
		"hasCpSection": false,
		"changelogUrl": "https://raw.githubusercontent.com/KuriousAgency/commerce-alipay/master/CHANGELOG.md",
		"class": "kuriousagency\\commerce\\alipay\\Alipay"
	}
}
